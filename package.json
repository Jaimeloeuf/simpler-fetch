{
  "name": "simpler-fetch",
  "version": "10.2.0",
  "description": "Simple and Easy to use fetch API abstraction with zero dependency that is super small!",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:watch": "npx tsc -w",
    "build:delete": "npx rimraf ./dist tsconfig.tsbuildinfo",
    "build:rollup": "npx rollup --config",
    "build": "npm run build:delete && npm run build:rollup",
    "prepack": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jaimeloeuf/simpler-fetch.git"
  },
  "keywords": [
    "simpler",
    "simple",
    "fetch",
    "http",
    "api",
    "easy",
    "oop",
    "builder pattern",
    "chainable",
    "typescript",
    "type-safe"
  ],
  "author": "JJ",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jaimeloeuf/simpler-fetch/issues"
  },
  "homepage": "https://github.com/Jaimeloeuf/simpler-fetch#readme",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.3",
    "@types/node": "^18.16.3",
    "rimraf": "^5.0.1",
    "rollup": "^3.23.1",
    "rollup-plugin-ts": "^3.2.0",
    "typescript": "^5.1.3",
    "zod": "^3.21.4"
  }
}