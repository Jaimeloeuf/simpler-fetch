{
  "name": "simpler-fetch",
  "version": "6.1.1",
  "description": "Easy to use, zero dependency abstraction over window.fetch",
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "build:watch": "npx tsc -w",
    "build:default": "npx tsc",
    "build:nocomments": "npx tsc -p ./tsconfig.nocomments.json",
    "build": "npm run build:default && npm run build:nocomments"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Enkel-Digital/simpler-fetch.git"
  },
  "keywords": [
    "simpler",
    "fetch",
    "easy",
    "simple",
    "oop",
    "chainable",
    "functional",
    "curried"
  ],
  "author": "JJ",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Enkel-Digital/simpler-fetch/issues"
  },
  "homepage": "https://github.com/Enkel-Digital/simpler-fetch#readme",
  "devDependencies": {
    "@types/node": "^17.0.23",
    "typescript": "^4.6.3"
  }
}
